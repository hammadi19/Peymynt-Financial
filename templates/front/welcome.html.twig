{% block javascripts %}


    <script>


        function validateEmail(email) {
            var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return re.test(String(email).toLowerCase());
        }

        function do_signup(){

            var errFLAG = false;
            var email_ref = $("#signupForm #signupEmail");
            var password_ref = $("#signupForm #signupPassword");
            var rpt_password_ref = $("#signupForm #rptsignupPassword");

            if(!validateEmail(email_ref.val())){
                //email_ref.addClass("error-class");
                email_ref.siblings("span").text("*Please provide valid info in above field.");
                errFLAG = true;
            }else{
                //email_ref.removeClass("error-class");
                email_ref.siblings("span").text("");
            }

            if(password_ref.val() == ""){
                //password_ref.addClass("error-class");
                password_ref.siblings("span").text("*Please provide valid info in above field.");
                errFLAG = true;
            }else{
                //password_ref.removeClass("error-class");
                password_ref.siblings("span").text("");
            }

            if(rpt_password_ref.val() == ""){
                //rpt_password_ref.addClass("error-class");
                rpt_password_ref.siblings("span").text("*Please provide valid info in above field.");
                errFLAG = true;
            }else{
                //rpt_password_ref.removeClass("error-class");
                rpt_password_ref.siblings("span").text("");
            }

            if(errFLAG){
                clearNotifications();
                //$("#signupError").text("Invalid info in highlighted fields.");
                return false;
            }else{
                clearNotifications();
                var url = '{{ path('app_user_signup') }}';
                $.ajax({type: "POST", url: url, data: {"formSignup":"true", "email":email_ref.val(), "password":password_ref.val(), "rptPassword":rpt_password_ref.val()}, success: function(data){
                    var objData = jQuery.parseJSON(data);
                    if(objData.code == 200){
                        successClear();
                        clearSignupFields();
                        $("#signupSuccess").html("Thank You for registering with us.");
                    }else{
                        $("#signupError").text(objData.message);
                    }
                }});
            }

            return false;
        }



        $("#signupFormButton").on("click", function(){
            $("#signupForm").submit();
        });
        $("#signinFormButton").on("click", function(){
            $("#signInForm").submit();
        });


        function clearSignupFields(){
            $("#signupForm input").each(function(index, elem){
                $(elem).val("");
            });
        }
        function clearNotifications(){
            $("#signupError").text("");
            $("#signupSuccess").text("");
        }

        function successClear(){
            $("#signupSubmitButton").addClass("d-none");
            $(".signupFormsCollection").addClass("d-none");
        }



        function do_signin(){
            var errFLAG = false;
            var email_ref = $("#signInForm #signinEmail");
            var password_ref = $("#signInForm #signinPassword");


            if(email_ref.val() == ""){
                email_ref.siblings("span").text("*Please provide valid info in above field.");
                //email_ref.addClass("error-class");
                errFLAG = true;
            }else{
                email_ref.siblings("span").text("");
                //email_ref.removeClass("error-class");
            }
            if(password_ref.val() == ""){
                password_ref.siblings("span").text("*Please provide valid info in above field.");
                //password_ref.addClass("error-class");
                errFLAG = true;
            }else{
                password_ref.siblings("span").text("");
                //password_ref.removeClass("error-class");
            }


            if(errFLAG){
                //$("#signInError").text("Invalid info in highlighted fields.");
                return false;
            }else{
                $("#loginButton").text("Please wait...").prop("disabled", true);
                var url = '{{ path('app_secure_login_check') }}';
                $.ajax({type: "POST", url: url, data: {"_username":email_ref.val(), "_password":password_ref.val()}, success: function(data){

                    if(data.success){
                        var redirectSuccessURL = '{{ path('app_user_dashboard') }}';
                        window.location.href = redirectSuccessURL;
                        $("#signInSuccess").html("Login Successful.");
                        $("#loginButton").text("Login");
                    }else{
                        $("#signInError").text(data.message);
                        $("#loginButton").text("Login").prop("disabled", false);
                    }

                }});
            }

            return false;
        }

    </script>

