                                                <p>{{ msg }}</p>
                                            {% endfor %}
                                            <a class="close"></a>
                                        </div>
                                    {% endif %}
                                    {% if errors %}
                                        <div class="notification error closeable">
                                            <ul style="margin-bottom:0px;">
                                                {% for error in errors %}
                                                    <li style="color: darkred">{{ error }}</li>
                                                {% endfor %}
                                            </ul>
                                            <a class="close"></a>
                                        </div>
